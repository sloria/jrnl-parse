{
  "name": "jrnl-parse",
  "version": "0.2.0",
  "description": "Parses jrnl (jrnl.sh) files. Works in both Node.js and the browser.",
  "scripts": {
    "test": "npm run lint && jest",
    "release": "np",
    "lint": "eslint ."
  },
  "files": [
    "index.js"
  ],
  "repository": "sloria/jrnl-parse",
  "authors": [
    "Steven Loria (https://github.com/sloria)"
  ],
  "license": "MIT",
  "keywords": [
    "jrnl",
    "parser",
    "jrnl.sh"
  ],
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-jest": "^22.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.1",
    "lint-staged": "^8.0.4",
    "np": "^4.0.2",
    "prettier": "^1.13.7"
  },
  "dependencies": {
    "date-fns": "^1.29.0"
  },
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
