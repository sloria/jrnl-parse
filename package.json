{
  "name": "jrnl-parse",
  "version": "0.4.0",
  "description": "Parses jrnl (jrnl.sh) files. Works in both Node.js and the browser.",
  "scripts": {
    "test": "npm run lint && jest",
    "release": "np",
    "lint": "eslint ."
  },
  "files": [
    "index.js"
  ],
  "repository": "sloria/jrnl-parse",
  "authors": [
    "Steven Loria (https://github.com/sloria)"
  ],
  "license": "MIT",
  "keywords": [
    "jrnl",
    "parser",
    "jrnl.sh"
  ],
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^26.5.0",
    "lint-staged": "^11.1.1",
    "np": "^6.2.1",
    "prettier": "^1.13.7"
  },
  "dependencies": {
    "date-fns": "^2.12.0"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
